{
  "name": "OFX",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest --config jest.config.json --no-cache",
    "test-watch": "jest --config jest.config.json --no-coverage --no-cache --watch",
    "test-coverage": "jest --config jest.config.json --coverage --no-cache",
    "lint": "eslint src/",
    "run:ios": "react-native run-ios",
    "run:droid": "react-native run-android",
    "run:release": "react-native run-ios && react-native run-android --variant=release",
    "apk": "cd android && ./gradlew assembleRelease",
    "atest:droid": "cd automationTest && mvn test -Dplatform=android",
    "atest:ios": "cd automationTest && mvn test -Dplatform=ios",
    "run:droid-release": "react-native run-android --variant=release",
    "clear:cache": "watchman watch-del-all && rm -rf $TMPDIR/react-* && rm -rf node_modules/ && npm cache clean --force && npm install ",
    "clear:droid": "del %appdata%\\Temp\\react-native-* & cd android & gradlew clean & cd .. & del node_modules/ & npm cache clean --force & npm install"
  },
  "dependencies": {
    "color-convert": "1.9.0",
    "create-react-class": "15.6.2",
    "crypto-js": "3.1.9-1",
    "enzyme": "3.1.0",
    "enzyme-to-json": "3.1.2",
    "errno": "0.1.4",
    "immutable": "3.8.2",
    "lodash": "4.17.4",
    "moment": "2.19.1",
    "phoneformat-react-native": "1.0.3",
    "prop-types": "15.6.0",
    "react-native-keyboard-aware-scroll-view": "^0.4.1",
    "react": "16.0.0-beta.5",
    "react-addons-test-utils": "15.6.2",
    "react-native": "0.49.0",
    "react-native-code-push": "5.1.3-beta",
    "react-native-fabric": "0.5.1",
    "react-native-i18n": "2.0.7",
    "react-native-masked-text": "1.6.2",
    "react-navigation": "1.0.0-beta.13",
    "react-redux": "5.0.6",
    "react-timer-mixin": "0.13.3",
    "realm": "1.13.0",
    "redux": "3.7.2",
    "redux-actions": "2.2.1",
    "redux-devtools-extension": "2.13.2",
    "redux-form": "7.1.2",
    "redux-promise-middleware": "4.4.1",
    "redux-saga": "0.15.6",
    "redux-thunk": "2.2.0"
  },
  "devDependencies": {
    "babel-jest": "21.2.0",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-preset-react-native": "4.0.0",
    "babel-preset-react-native-stage-0": "1.0.1",
    "enzyme-adapter-react-16": "1.0.1",
    "enzyme-matchers": "4.0.1",
    "eslint": "4.9.0",
    "eslint-config-airbnb": "16.0.0",
    "eslint-config-standard": "10.2.1",
    "eslint-loader": "1.9.0",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-jest": "21.2.0",
    "eslint-plugin-json": "1.2.0",
    "eslint-plugin-jsx-a11y": "6.0.2",
    "eslint-plugin-promise": "3.6.0",
    "eslint-plugin-react": "7.4.0",
    "eslint-plugin-react-native": "3.1.0",
    "eslint-plugin-standard": "3.0.1",
    "jest": "21.2.1",
    "jest-bamboo-formatter": "1.0.1",
    "jest-cli": "21.2.1",
    "jest-enzyme": "4.0.1",
    "react-dom": "16.0.0-rc.3",
    "react-native-debugger-open": "0.3.15",
